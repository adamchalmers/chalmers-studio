@settings(defaultLengthUnit = cm)

import carveHole, counterbore, holeProfile, drill, blind from "hole_lib.kcl"

chalmersCorpStdHole = holeProfile(
  bottom = drill(bitAngle = 118deg),
  body = blind(depth = 10, diameter = 4),
  top = counterbore(diameter = 5, depth = 2),
)

carveHole(chalmersCorpStdHole)
